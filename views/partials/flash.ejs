
<% if(success && success.length) {%>
    <div class="alert alert-success alert-dismissible fade show col-md-6 offset-md-3 col-xl-4 offset-xl-4 mt-2"role="alert">
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
        </button>
    </div>
    <% } %>
    
    <% if(error && error.length) {%>
    <div class="alert alert-danger alert-dismissible fade show col-md-6 offset-md-3 col-xl-4 offset-xl-4 mt-2" role="alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
        </button>
    </div>
    <% } %>
